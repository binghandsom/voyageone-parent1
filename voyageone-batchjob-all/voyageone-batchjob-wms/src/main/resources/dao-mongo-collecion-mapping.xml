<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="mongoCollectionMapping" class="com.voyageone.base.dao.mongodb.MongoCollectionMapping">
        <property name="collectionNameMap">
            <map>
                <entry key="CmsBtFeedInfoModel"						value="cms_bt_feed_info" />
                <entry key="CmsBtPlatformMappingModel"			value="cms_bt_platform_mapping" />
                <entry key="CmsBtProductModel"						value="cms_bt_product" />
                <entry key="CmsBtProductLogModel"					value="cms_bt_product_log" />
                <entry key="CmsBtTranErrInfoModel"					value="cms_bt_tran_err_info" />
                <entry key="CmsMtCategorySchemaModel"				value="cms_mt_category_schema" />
                <entry key="CmsMtCategoryTreeModel"				value="cms_mt_category_tree" />
                <entry key="CmsMtFeedCategorySchemaModel"			value="cms_mt_feed_category_schema" />
                <entry key="CmsMtFeedCategoryTreeModel"			value="cms_mt_feed_category_tree" />
                <entry key="CmsMtPlatformCategorySchemaModel"	value="cms_mt_platform_category_schema" />
                <entry key="CmsMtPlatformCategoryTreeModel"		value="cms_mt_platform_category_tree" />
                <entry key="CmsMtComSchemaModel"                value="cms_mt_common_schema" />
            </map>
        </property>
    </bean>

</beans>