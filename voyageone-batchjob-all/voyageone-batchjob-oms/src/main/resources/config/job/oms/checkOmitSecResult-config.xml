<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mongo="http://www.springframework.org/schema/data/mongo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
	<!-- ============================PA=================================================== --> 
   <bean id="CheckOmitSecResultPATaskJob"
		class="com.voyageone.batch.oms.job.CheckOmitSecResultJob">
		<property name="taskCheck">
			<value>CheckOmitSecResultPAJob</value>
		</property> 
		<property name="channelId">
			<value>002</value>
		</property> 
				
	</bean>	
	<bean id="CheckOmitSecResultPATask"
		class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">
		<property name="targetObject">
			<ref local="CheckOmitSecResultPATaskJob" />
		</property>
		<property name="targetMethod">
			<value>run</value>
		</property>
	</bean>	
	<bean id="CheckOmitSecResultPATaskContri" 
		name="CheckOmitSecResultPATaskContri"
		class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
		<property name="jobDetail">
			<ref bean="CheckOmitSecResultPATask" />
		</property>
		<property name="cronExpression">
			<value>${quartz.order.checkomit.transfer}</value>
		</property>
	</bean>
