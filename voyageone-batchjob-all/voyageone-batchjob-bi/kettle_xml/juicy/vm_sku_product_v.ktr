    <sql>INSERT INTO bi_s004.vm_product_all&#xd;&#xa;&#x28;&#xd;&#xa;	code,&#xd;&#xa;	name,&#xd;&#xa;	sname,&#xd;&#xa;	name_cn,&#xd;&#xa;	sname_cn,&#xd;&#xa;	model_id,&#xd;&#xa;	model_code,&#xd;&#xa;	brand_id,&#xd;&#xa;	color_id,&#xd;&#xa;	material_id,&#xd;&#xa;	enable,&#xd;&#xa;	create_time,&#xd;&#xa;	update_time&#xd;&#xa;&#x29;&#xd;&#xa;SELECT&#xd;&#xa;	p.Code code,&#xd;&#xa;	ifnull&#x28;ifnull&#x28;p.name, m.name&#x29;, &#x27;unknown&#x27;&#x29; name,&#xd;&#xa;	ifnull&#x28;ifnull&#x28;p.name, m.name&#x29;, &#x27;unknown&#x27;&#x29; sname,&#xd;&#xa;	ifnull&#x28;ifnull&#x28;p.name, m.name&#x29;, &#x27;&#x672a;&#x77e5;&#x27;&#x29; name_cn,&#xd;&#xa;	ifnull&#x28;ifnull&#x28;p.name, m.name&#x29;, &#x27;&#x672a;&#x77e5;&#x27;&#x29; sname_cn,&#xd;&#xa;	ifnull&#x28;vm_model.id, 0&#x29; model_id,&#xd;&#xa;	ifnull&#x28;m.model,0&#x29; model_code,&#xd;&#xa;	ifnull&#x28;vm_b.id, 0&#x29; brand_id,&#xd;&#xa;	ifnull&#x28;vm_c.id, 0&#x29; color_id,&#xd;&#xa;	vm_m.id material_id,&#xd;&#xa;	&#x27;1&#x27; enable,&#xd;&#xa;	now&#x28;&#x29; create_time,&#xd;&#xa;	now&#x28;&#x29; update_time&#xd;&#xa;FROM juicy_products p&#xd;&#xa;LEFT JOIN juicy_modelproducts mp on mp.productid&#x3d;p.Id&#xd;&#xa;LEFT JOIN juicy_models m on m.id&#x3d;mp.modelid&#xd;&#xa;LEFT JOIN juicy_brands b on b.id&#x3d;m.brandId&#xd;&#xa;LEFT JOIN bi_s004.vm_brand vm_b on vm_b.name&#x3d;b.name&#xd;&#xa;LEFT JOIN bi_s004.vm_color vm_c on vm_c.name&#x3d;p.color&#xd;&#xa;LEFT JOIN bi_s004.vm_model_all vm_model on vm_model.code&#x3d;m.model&#xd;&#xa;LEFT JOIN bi_s004.vm_material vm_m on vm_m.code&#x3d;p.MaterialFabricTypeId_01&#xd;&#xa;ON DUPLICATE KEY UPDATE&#xd;&#xa;name&#x3d;VALUES&#x28;name&#x29;,&#xd;&#xa;sname&#x3d;VALUES&#x28;sname&#x29;,&#xd;&#xa;name_cn&#x3d;VALUES&#x28;name_cn&#x29;,&#xd;&#xa;sname_cn&#x3d;VALUES&#x28;sname_cn&#x29;,&#xd;&#xa;model_id&#x3d;VALUES&#x28;model_id&#x29;,&#xd;&#xa;model_code&#x3d;VALUES&#x28;model_code&#x29;,&#xd;&#xa;brand_id&#x3d;VALUES&#x28;brand_id&#x29;,&#xd;&#xa;color_id&#x3d;VALUES&#x28;color_id&#x29;,&#xd;&#xa;material_id&#x3d;VALUES&#x28;material_id&#x29;,&#xd;&#xa;update_time&#x3d;VALUES&#x28;update_time&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;--update main color&#xd;&#xa;TRUNCATE TABLE bi_s004.vm_color_tmp&#x3b;&#xd;&#xa;INSERT INTO bi_s004.vm_color_tmp&#xd;&#xa;SELECT color.id, IFNULL&#x28;define.id,0&#x29;, IFNULL&#x28;FIND_IN_SET&#x28;UPPER&#x28;define.name&#x29;,  color.rep_name&#x29;, 0&#x29;&#xd;&#xa;FROM&#xd;&#xa;&#x28;&#xd;&#xa;  SELECT id, name, REPLACE&#x28;UPPER&#x28;bi_master.REGEX_REPLACE&#x28;&#x27;&#x5c;&#x5c;&#x28;&#x7c;&#x5c;&#x5c;&#x29;&#x7c;&#x2f;&#x7c;,&#x7c;-&#x27;, &#x27; &#x27;, name&#x29;&#x29;, &#x27; &#x27;, &#x27;,&#x27;&#x29; rep_name  &#xd;&#xa;  FROM bi_s004.vm_color&#xd;&#xa;&#x29; color&#xd;&#xa;LEFT JOIN bi_s004.vm_color_define define &#xd;&#xa;ON FIND_IN_SET&#x28;UPPER&#x28;define.name&#x29;,  color.rep_name&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;TRUNCATE TABLE bi_s004.vm_product_color_tmp&#x3b;&#xd;&#xa;INSERT INTO bi_s004.vm_product_color_tmp&#xd;&#xa;SELECT v.id, tt.define_id FROM bi_s004.vm_product_all v&#xd;&#xa;INNER JOIN &#xd;&#xa;&#x28;SELECT v.&#x2a; FROM bi_s004.vm_color_tmp v&#xd;&#xa;	INNER JOIN &#x28;SELECT id, min&#x28;find_index&#x29; find_index FROM bi_s004.vm_color_tmp WHERE define_id&#x3c;&#x3e;0 AND find_index&#x3c;&#x3e;0 group by id&#x29; t &#xd;&#xa;	ON t.id&#x3d;v.id and t.find_index&#x3d;v.find_index&#xd;&#xa;WHERE v.define_id&#x3c;&#x3e;0 and v.find_index&#x3c;&#x3e;0&#x29; tt on tt.id&#x3d;v.color_id&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE bi_s004.vm_product_all v&#xd;&#xa;INNER JOIN bi_s004.vm_product_color_tmp t ON t.id&#x3d;v.id&#xd;&#xa;SET v.main_color_id &#x3d; t.main_color_id</sql>
