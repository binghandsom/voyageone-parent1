    <sql>INSERT INTO bi_s008.vt_stock_sku&#xd;&#xa;&#x28;date&#xd;&#xa;,channel_id&#xd;&#xa;,house_id&#xd;&#xa;,sku_id&#xd;&#xa;,product_id&#xd;&#xa;,brand_id&#xd;&#xa;,main_color_id&#xd;&#xa;,color_id&#xd;&#xa;,size_id&#xd;&#xa;,onsale_qty&#xd;&#xa;,salable_qty&#xd;&#xa;,whole_qty&#xd;&#xa;&#x29;&#xd;&#xa;SELECT &#xd;&#xa; replace&#x28;&#x27;&#x24;&#x7b;start_date&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29; data,&#xd;&#xa; &#x27;9&#x27; channel_id,&#xd;&#xa; stock.store_id house_id,&#xd;&#xa; sku.id sku_id,&#xd;&#xa; sku.product_id product_id,&#xd;&#xa; sku.brand_id brand_id,&#xd;&#xa; sku.main_color_id main_color_id,&#xd;&#xa; sku.color_id color_id,&#xd;&#xa; sku.size_id size_id,&#xd;&#xa; &#x27;0&#x27; onsale_qty,&#xd;&#xa; stock.currInv salable_qty,&#xd;&#xa; stock.currInv whole_qty&#xd;&#xa;FROM&#xd;&#xa;&#x28;&#xd;&#xa;SELECT&#xd;&#xa; transfer.store_id,&#xd;&#xa; item.transfer_sku sku,&#xd;&#xa; IFNULL&#x28;SUM&#x28;item.calc_qty&#x29;, 0&#x29; currInv&#xd;&#xa;FROM synship_wms_bt_transfer transfer&#xd;&#xa;INNER JOIN synship_wms_bt_transfer_item item ON transfer.transfer_id &#x3d; item.transfer_id&#xd;&#xa;INNER JOIN&#xd;&#xa;  &#x28;&#xd;&#xa;	SELECT max&#x28;transfer_item_id&#x29; transfer_item_id&#xd;&#xa;	FROM synship_wms_bt_transfer_item item&#xd;&#xa;	INNER JOIN synship_wms_bt_transfer transfer on transfer.transfer_id&#x3d;item.transfer_id and item.calc_qty&#x3e;&#x3d;0 and transfer_type&#x3d;&#x27;5&#x27;&#xd;&#xa;	WHERE transfer.order_channel_id &#x3d; &#x27;008&#x27;&#xd;&#xa;	 AND transfer.created&#x3c;&#x3d;DATE_SUB&#x28;CONCAT&#x28;&#x27;&#x24;&#x7b;start_date&#x7d;&#x27;,&#x27; 23&#x3a;59&#x3a;59&#x27;&#x29;, INTERVAL 8 HOUR&#x29;&#xd;&#xa;	GROUP BY transfer.store_id, item.transfer_sku&#xd;&#xa;  &#x29; item_id ON item_id.transfer_item_id&#x3d;item.transfer_item_id&#xd;&#xa;  GROUP BY transfer.store_id, item.transfer_sku&#xd;&#xa;&#x29;  stock&#xd;&#xa;INNER JOIN bi_s008.vm_sku_all sku ON sku.sku_code&#x3d;stock.sku&#xd;&#xa;INNER JOIN bi_s008.vs_sales_product_iid iid on iid.product_code&#x3d;sku.product_code&#xd;&#xa;ON DUPLICATE KEY UPDATE&#xd;&#xa; onsale_qty&#x3d;VALUES&#x28;onsale_qty&#x29;&#xd;&#xa;,salable_qty&#x3d;VALUES&#x28;salable_qty&#x29;&#xd;&#xa;,whole_qty&#x3d;VALUES&#x28;whole_qty&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO bi_s008.vt_stock_sku&#xd;&#xa;&#x28;date&#xd;&#xa;,channel_id&#xd;&#xa;,house_id&#xd;&#xa;,sku_id&#xd;&#xa;,product_id&#xd;&#xa;,brand_id&#xd;&#xa;,main_color_id&#xd;&#xa;,color_id&#xd;&#xa;,size_id&#xd;&#xa;,onsale_qty&#xd;&#xa;&#x29;&#xd;&#xa;SELECT&#xd;&#xa; replace&#x28;&#x27;&#x24;&#x7b;start_date&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29; data,&#xd;&#xa; &#x27;9&#x27; channel_id,&#xd;&#xa; stock.store_id house_id,&#xd;&#xa; sku.id sku_id,&#xd;&#xa; sku.product_id product_id,&#xd;&#xa; sku.brand_id brand_id,&#xd;&#xa; sku.main_color_id main_color_id,&#xd;&#xa; sku.color_id color_id,&#xd;&#xa; sku.size_id size_id,&#xd;&#xa; stock.currInv onsale_qty&#xd;&#xa;FROM&#xd;&#xa;&#x28;&#xd;&#xa;SELECT&#xd;&#xa; transfer.store_id,&#xd;&#xa; item.transfer_sku sku,&#xd;&#xa; IFNULL&#x28;SUM&#x28;item.calc_qty&#x29;, 0&#x29; currInv&#xd;&#xa;FROM synship_wms_bt_transfer transfer&#xd;&#xa;INNER JOIN synship_wms_bt_transfer_item item ON transfer.transfer_id &#x3d; item.transfer_id&#xd;&#xa;INNER JOIN&#xd;&#xa;  &#x28;&#xd;&#xa;	SELECT max&#x28;transfer_item_id&#x29; transfer_item_id&#xd;&#xa;	FROM synship_wms_bt_transfer_item item&#xd;&#xa;	INNER JOIN synship_wms_bt_transfer transfer on transfer.transfer_id&#x3d;item.transfer_id and item.calc_qty&#x3e;&#x3d;0 and transfer_type&#x3d;&#x27;5&#x27;&#xd;&#xa;	WHERE transfer.order_channel_id &#x3d; &#x27;008&#x27;&#xd;&#xa;	 AND transfer.created&#x3c;&#x3d;DATE_SUB&#x28;CONCAT&#x28;&#x27;&#x24;&#x7b;start_date&#x7d;&#x27;,&#x27; 23&#x3a;59&#x3a;59&#x27;&#x29;, INTERVAL 8 HOUR&#x29;&#xd;&#xa;	GROUP BY transfer.store_id, item.transfer_sku&#xd;&#xa;  &#x29; item_id ON item_id.transfer_item_id&#x3d;item.transfer_item_id&#xd;&#xa;  GROUP BY transfer.store_id, item.transfer_sku&#xd;&#xa;&#x29;  stock&#xd;&#xa;INNER JOIN bi_s008.vm_sku_all sku ON sku.sku_code&#x3d;stock.sku&#xd;&#xa;INNER JOIN&#xd;&#xa; &#x28;SELECT DISTINCT product_code FROM bi_s008.vs_sales_product_iid iid INNER JOIN bi_s008.vs_view_sales_product_lift_cycle lift_cycle ON lift_cycle.num_iid&#x3d;iid.num_iid and lift_cycle.approve_status&#x3d;&#x27;onsale&#x27;&#x29; iid&#xd;&#xa;ON iid.product_code&#x3d;sku.product_code&#xd;&#xa;ON DUPLICATE KEY UPDATE&#xd;&#xa;onsale_qty&#x3d;VALUES&#x28;onsale_qty&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;delete FROM bi_s008.vt_stock_sku where date&#x3d;replace&#x28;&#x27;&#x24;&#x7b;start_date&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29; and onsale_qty&#x3d;0 and salable_qty&#x3d;0 and whole_qty&#x3d;0&#x3b;</sql>
