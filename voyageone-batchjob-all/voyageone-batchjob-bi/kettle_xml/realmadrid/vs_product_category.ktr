    <replace_variables>Y</replace_variables>
    <quoteString>N</quoteString>
    <sql>INSERT INTO vm_product_category&#xd;&#xa;&#x28;&#xd;&#xa;product_id,&#xd;&#xa;category_id,&#xd;&#xa;category1_id,&#xd;&#xa;category2_id,&#xd;&#xa;category3_id&#xd;&#xa;&#x29;&#xd;&#xa;SELECT&#xd;&#xa;	product.id product_id,&#xd;&#xa;	ifnull&#x28;cat.cid, 0&#x29; category_id,&#xd;&#xa;	ifnull&#x28;cat.id1, 0&#x29; category1_id,&#xd;&#xa;	ifnull&#x28;cat.id2, 0&#x29; category2_id,&#xd;&#xa;	ifnull&#x28;cat.id3, 0&#x29; category3_id&#xd;&#xa;FROM vm_product product&#xd;&#xa;LEFT JOIN&#xd;&#xa;&#x28;&#xd;&#xa;	SELECT&#xd;&#xa;	  iid.product_code product_code,&#xd;&#xa;	  ifnull&#x28;max&#x28;cid&#x29;, 0&#x29; category_code&#xd;&#xa;	FROM vs_view_sales_product_lift_cycle v&#xd;&#xa;	INNER JOIN vs_sales_product_iid iid on iid.num_iid&#x3d;v.num_iid&#xd;&#xa;	GROUP BY iid.product_code&#xd;&#xa;&#x29; cat_product ON cat_product.product_code&#x3d;product.code&#xd;&#xa;LEFT JOIN&#xd;&#xa;&#x28;&#xd;&#xa;	SELECT&#xd;&#xa;	vm_cat3.id cid,&#xd;&#xa;	vm_cat3.code code,&#xd;&#xa;	vm_cat1.id id1,&#xd;&#xa;	vm_cat2.id id2,&#xd;&#xa;	vm_cat3.id id3&#xd;&#xa;	FROM vm_category vm_cat3&#xd;&#xa;	INNER JOIN vm_category vm_cat2 ON vm_cat2.id&#x3d;vm_cat3.parent_id&#xd;&#xa;	INNER JOIN vm_category vm_cat1 ON vm_cat1.id&#x3d;vm_cat2.parent_id&#xd;&#xa;	WHERE vm_cat3.level&#x3d;3&#xd;&#xa;	UNION ALL&#xd;&#xa;	SELECT&#xd;&#xa;	vm_cat2.id cid,&#xd;&#xa;	vm_cat2.code code,&#xd;&#xa;	vm_cat1.id id1,&#xd;&#xa;	vm_cat2.id id2,&#xd;&#xa;	&#x27;0&#x27; id3&#xd;&#xa;	FROM vm_category vm_cat2&#xd;&#xa;	INNER JOIN vm_category vm_cat1 ON vm_cat1.id&#x3d;vm_cat2.parent_id&#xd;&#xa;	WHERE vm_cat2.level&#x3d;2&#xd;&#xa;&#x29; cat ON cat.code&#x3d;cat_product.category_code&#xd;&#xa;ON DUPLICATE KEY UPDATE&#xd;&#xa;category_id&#x3d;VALUES&#x28;category_id&#x29;,&#xd;&#xa;category1_id&#x3d;VALUES&#x28;category1_id&#x29;,&#xd;&#xa;category2_id&#x3d;VALUES&#x28;category2_id&#x29;,&#xd;&#xa;category3_id&#x3d;VALUES&#x28;category3_id&#x29;&#x3b;</sql>
