    <sql>DELETE FROM bi_s005.vt_sales_model WHERE date&#x3e;&#x3d;replace&#x28;&#x27;&#x24;&#x7b;start_date&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT INTO bi_s005.vt_sales_model&#xd;&#xa;&#x28;&#xd;&#xa;	date,&#xd;&#xa;	hour,&#xd;&#xa;	buy_channel,&#xd;&#xa;	shop_id,&#xd;&#xa;	model_id,&#xd;&#xa;	brand_id,&#xd;&#xa;	region_id,&#xd;&#xa;	qty,&#xd;&#xa;	amt,&#xd;&#xa;	income,&#xd;&#xa;	cost,&#xd;&#xa;	orders,&#xd;&#xa;	buyer,&#xd;&#xa;	rep_buyer,&#xd;&#xa;	buy_qty,&#xd;&#xa;	buy_amt,&#xd;&#xa;	ret_qty,&#xd;&#xa;	ret_amt&#xd;&#xa;&#x29;&#xd;&#xa;SELECT&#xd;&#xa; tra.date date,&#xd;&#xa; tra.hour hour,&#xd;&#xa; &#x27;0&#x27; buy_channel,&#xd;&#xa; tra.shop_id shop_id,&#xd;&#xa; tra.model_id,&#xd;&#xa; tra.brand_id,&#xd;&#xa; &#x27;0&#x27; region_id,&#xd;&#xa; IFNULL&#x28;SUM&#x28;tra.qty&#x29;,0&#x29; qty,&#xd;&#xa; IFNULL&#x28;SUM&#x28;tra.amt&#x29;,0&#x29; amt,&#xd;&#xa; IFNULL&#x28;SUM&#x28;tra.amt&#x29;,0&#x29; income,&#xd;&#xa; IFNULL&#x28;SUM&#x28;tra.cost&#x29;,0&#x29; cost,&#xd;&#xa; COUNT&#x28;distinct tra.order_number&#x29; orders,&#xd;&#xa; COUNT&#x28;distinct tra.customer_id&#x29; buyer,&#xd;&#xa; &#x27;0&#x27; rep_buyer,&#xd;&#xa; &#x27;0&#x27; buy_qty,&#xd;&#xa; &#x27;0&#x27; buy_amt,&#xd;&#xa; IFNULL&#x28;SUM&#x28;tra.ret_qty&#x29;,0&#x29; ret_qty,&#xd;&#xa; IFNULL&#x28;SUM&#x28;tra.ret_amt&#x29;,0&#x29; ret_amt&#xd;&#xa;FROM&#xd;&#xa;&#x28;&#xd;&#xa;	SELECT&#xd;&#xa;		ord.order_number,&#xd;&#xa;		det.sku,&#xd;&#xa;		IFNULL&#x28;v.id,0&#x29; sku_id,&#xd;&#xa;		IFNULL&#x28;v.product_id,0&#x29; product_id,&#xd;&#xa;		IFNULL&#x28;p.model_id,0&#x29; model_id,&#xd;&#xa;		IFNULL&#x28;v.brand_id,0&#x29; brand_id,&#xd;&#xa;		IFNULL&#x28;v.main_color_id,0&#x29; main_color_id,&#xd;&#xa;		IFNULL&#x28;v.color_id,0&#x29; color_id,&#xd;&#xa;		IFNULL&#x28;v.size_id,0&#x29; size_id,&#xd;&#xa;		IFNULL&#x28;SUM&#x28;v.cost_price&#x2a;det.qty&#x29;,0&#x29; cost,&#xd;&#xa;		ord.customer_id,&#xd;&#xa;		DATE_FORMAT&#x28;DATE_ADD&#x28;order_date_time, INTERVAL 8 HOUR&#x29;, &#x27;&#x25;Y&#x25;m&#x25;d&#x27;&#x29; AS date,&#xd;&#xa;		DATE_FORMAT&#x28;DATE_ADD&#x28;order_date_time, INTERVAL 8 HOUR&#x29;, &#x27;&#x25;H&#x27;&#x29; AS hour,&#xd;&#xa;		CASE IF&#x28;cart_id&#x3d;0, 20,cart_id&#x29;&#xd;&#xa;			WHEN 20 THEN 10&#xd;&#xa;			WHEN 25 THEN 11&#xd;&#xa;		ELSE 10&#xd;&#xa;		END  shop_id,&#xd;&#xa;		sum&#x28;det.qty&#x29; qty,&#xd;&#xa;		sum&#x28;det.amt&#x29; amt,&#xd;&#xa;		sum&#x28;det.ret_qty&#x29; ret_qty,&#xd;&#xa;		sum&#x28;det.ret_amt&#x29; ret_amt&#xd;&#xa;	FROM spalding_oms_bt_orders_tmp ord&#xd;&#xa;	INNER JOIN&#xd;&#xa;	&#x28;&#xd;&#xa;		SELECT&#xd;&#xa;		  detail.order_number order_number,&#xd;&#xa;		  detail.sku sku,&#xd;&#xa;		  SUM&#x28;CASE WHEN detail.status&#x3d;&#x27;Returned&#x27; THEN 0 ELSE detail.quantity_ordered-detail.quantity_returned END&#x29; qty,&#xd;&#xa;		  SUM&#x28;detail.quantity_returned&#x29; ret_qty,&#xd;&#xa;		  SUM&#x28;CASE WHEN detail.status&#x3d;&#x27;Returned&#x27; THEN 0 ELSE detail.price_per_unit&#x2a;&#x28;detail.quantity_ordered-detail.quantity_returned&#x29;&#x2a;&#x28;ifnull&#x28;surcharge_amt&#x2f;sales_amt, 0&#x29;&#x2b;1&#x29; END&#x29; amt,&#xd;&#xa;		  SUM&#x28;detail.price_per_unit&#x2a;&#x28;detail.quantity_returned&#x29;&#x2a;&#x28;IFNULL&#x28;&#x28;surcharge_amt-ret_amt2&#x29;&#x2f;ret_amt1, 0&#x29;&#x2b;1&#x29;&#x29; ret_amt&#xd;&#xa;		FROM spalding_oms_bt_order_details_tmp detail&#xd;&#xa;		INNER JOIN spalding_oms_order_details_amt_tmp surcharge ON surcharge.order_number&#x3d;detail.order_number&#xd;&#xa;		WHERE detail.adjustment&#x3d;0 AND detail.status not in &#x28;&#x27;Canceled&#x27;&#x29;&#xd;&#xa;		GROUP BY detail.order_number, detail.sku&#xd;&#xa;	&#x29; det ON det.order_number&#x3d;ord.order_number AND &#x28;det.amt&#x3c;&#x3e;0 or det.ret_amt&#x3c;&#x3e;0&#x29;&#xd;&#xa;	LEFT JOIN bi_s005.vm_sku_all v ON v.sku_code&#x3d;det.sku&#xd;&#xa;	LEFT JOIN bi_s005.vm_product_all p ON p.id&#x3d;v.product_id&#xd;&#xa;	GROUP BY order_number, det.sku&#xd;&#xa;&#x29; tra&#xd;&#xa;GROUP BY tra.date, tra.hour, tra.shop_id, tra.model_id&#xd;&#xa;ON DUPLICATE KEY UPDATE&#xd;&#xa;brand_id&#x3d;VALUES&#x28;brand_id&#x29;,&#xd;&#xa;region_id&#x3d;VALUES&#x28;region_id&#x29;,&#xd;&#xa;qty&#x3d;VALUES&#x28;qty&#x29;,&#xd;&#xa;amt&#x3d;VALUES&#x28;amt&#x29;,&#xd;&#xa;income&#x3d;VALUES&#x28;income&#x29;,&#xd;&#xa;cost&#x3d;VALUES&#x28;cost&#x29;,&#xd;&#xa;orders&#x3d;VALUES&#x28;orders&#x29;,&#xd;&#xa;buyer&#x3d;VALUES&#x28;buyer&#x29;,&#xd;&#xa;rep_buyer&#x3d;VALUES&#x28;rep_buyer&#x29;,&#xd;&#xa;buy_qty&#x3d;VALUES&#x28;buy_qty&#x29;,&#xd;&#xa;buy_amt&#x3d;VALUES&#x28;buy_amt&#x29;,&#xd;&#xa;ret_qty&#x3d;VALUES&#x28;ret_qty&#x29;,&#xd;&#xa;ret_amt&#x3d;VALUES&#x28;ret_amt&#x29;&#xd;&#xa;&#x3b;</sql>
