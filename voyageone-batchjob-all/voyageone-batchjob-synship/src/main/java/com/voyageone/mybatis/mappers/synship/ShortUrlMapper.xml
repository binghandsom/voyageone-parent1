<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.voyageone.batch.synship.sql">
    <select id="tt_short_url_selectBySourceOrderIdFromTtShorturl" resultType="com.voyageone.batch.synship.modelbean.ShortUrlBean">
        SELECT
            comment,
            ship_phone,
            ship_name,
            source_order_id,
            type,
            short_key,
            cart_id
        FROM Synship.tt_shorturl
        WHERE source_order_id = #{source_order_id} AND type = '1'
        limit 1
    </select>
</mapper>