<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.voyageone.wms.sql">
    <select id="wms_mt_stores_get_simple_all" resultType="com.voyageone.core.modelbean.ChannelStoreBean">
        select store_id, store_name from wms_mt_store
    </select>

    <select id="wms_mt_stores_get_channel_id" resultType="string">
        select order_channel_id from wms_mt_store where store_id = #{store_id}
    </select>

    <select id="wms_mt_stores_get_store_name" resultType="string">
        select store_name from wms_mt_store where store_id = #{store_id}
    </select>
</mapper>