<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.voyageone.wsdl.core.sql">

	<sql id="exception_column">
		url, 
		exception_type, 
		description, 
		date_time, 
		created,
		creater
	</sql>
	
	<select id="insertException" parameterType="core.ExceptionLogBean" resultType="int" >
		insert into wsdl_ct_exception_log 
			(<include refid="exception_column"/>)
		values 
			(
			#{url},
			#{exceptionType},
			#{description},
			#{dateTime},
			#{created},
			'WSDL'
			)
	</select>
	
</mapper>
