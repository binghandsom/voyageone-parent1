<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.voyageone.common.sql">

    <select id="select_feed_image" resultType="com.voyageone.cms.service.model.CmsBtFeedProductImageModel" parameterType="com.voyageone.cms.service.model.CmsBtFeedProductImageModel" >
        select
          channel_id,
          image_name,
          image_type_id,
          image_url,
          sent_flag,
          created,
          creater,
          modified,
          modifier
        from voyageone_cms2.cms_bt_feed_product_image
        <where>
            <if test="channelId != null and channelId !=''">
                channel_id = #{channelId,jdbcType=VARCHAR}
            </if>
            <if test="sentFlag != null and sentFlag !=''">
                sent_flag = #{sentFlag,jdbcType=INTEGER}
            </if>
            <if test="imageUrl != null and imageUrl !=''">
                image_url = #{imageUrl,jdbcType=VARCHAR}
            </if>
        </where>
    </select>
    <insert id="insert_feed_image" parameterType="com.voyageone.cms.service.model.CmsBtFeedProductImageModel" >
        insert IGNORE into voyageone_cms2.cms_bt_feed_product_image
        <trim prefix="(" suffix=")" suffixOverrides="," >
            <if test="channelId != null" >
                channel_id,
            </if>
            <if test="imageName != null" >
                image_name,
            </if>
            <if test="imageTypeId != null" >
                image_type_id,
            </if>
            <if test="imageUrl != null" >
                image_url,
            </if>
            <if test="sentFlag != null" >
                sent_flag,
            </if>
                created,
            <if test="creater != null" >
                creater,
            </if>
                modified,
            <if test="modifier != null" >
                modifier,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides="," >
            <if test="channelId != null" >
                #{channelId,jdbcType=VARCHAR},
            </if>
            <if test="imageName != null" >
                #{imageName,jdbcType=VARCHAR},
            </if>
            <if test="imageTypeId != null" >
                #{imageTypeId,jdbcType=INTEGER},
            </if>
            <if test="imageUrl != null" >
                #{imageUrl,jdbcType=VARCHAR},
            </if>
            <if test="sentFlag != null" >
                #{sentFlag,jdbcType=INTEGER},
            </if>
                now(),
            <if test="creater != null" >
                #{creater,jdbcType=VARCHAR},
            </if>
               now(),
            <if test="modifier != null" >
                #{modifier,jdbcType=VARCHAR},
            </if>
        </trim>
    </insert>

</mapper>
