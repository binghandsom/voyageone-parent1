<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.voyageone.service.dao.cms.CmsBtImagesDao">
    <resultMap id="ResultMap" type="CmsBtImagesModel">

    </resultMap>

    <select id="selectList" resultMap="ResultMap">

        select `id`,`channel_id`,`code`,`original_url`,`img_name`,`upd_flg`,`created`,`creater`,`modified`,`modifier`
        from cms_bt_images
        <where>
            <if test="id != null">
                AND `id` = #{id}
            </if>
            <if test="channelId != null">
                AND `channel_id` = #{channelId}
            </if>
            <if test="code != null">
                AND `code` = #{code}
            </if>
            <if test="originalUrl != null">
                AND `original_url` = #{originalUrl}
            </if>
            <if test="imgName != null">
                AND `img_name` = #{imgName}
            </if>
            <if test="updFlg != null">
                AND `upd_flg` = #{updFlg}
            </if>
            <if test="created != null">
                AND `created` = #{created}
            </if>
            <if test="creater != null">
                AND `creater` = #{creater}
            </if>
            <if test="modified != null">
                AND `modified` = #{modified}
            </if>
            <if test="modifier != null">
                AND `modifier` = #{modifier}
            </if>
        </where>
    </select>
    <select id="selectOne" resultMap="ResultMap">
        select `id`,`channel_id`,`code`,`original_url`,`img_name`,`upd_flg`,`created`,`creater`,`modified`,`modifier`
        from cms_bt_images
        <where>

            <if test="id != null">
                AND `id` = #{id}
            </if>

            <if test="channelId != null">
                AND `channel_id` = #{channelId}
            </if>

            <if test="code != null">
                AND `code` = #{code}
            </if>

            <if test="originalUrl != null">
                AND `original_url` = #{originalUrl}
            </if>

            <if test="imgName != null">
                AND `img_name` = #{imgName}
            </if>

            <if test="updFlg != null">
                AND `upd_flg` = #{updFlg}
            </if>

            <if test="created != null">
                AND `created` = #{created}
            </if>

            <if test="creater != null">
                AND `creater` = #{creater}
            </if>

            <if test="modified != null">
                AND `modified` = #{modified}
            </if>

            <if test="modifier != null">
                AND `modifier` = #{modifier}
            </if>
        </where>
        limit 1
    </select>
    <select id="select" resultMap="ResultMap">
        select `id`,`channel_id`,`code`,`original_url`,`img_name`,`upd_flg`,`created`,`creater`,`modified`,`modifier` from cms_bt_images where id=#{id}
    </select>

    <update id="update">
        update cms_bt_images
        set

            `channel_id`=#{channelId}
            ,`code`=#{code}
            ,`original_url`=#{originalUrl}
            ,`img_name`=#{imgName}
            ,`upd_flg`=#{updFlg}
            ,`modifier`=#{modifier}
         where id=#{id}   
   </update>

    <delete id="delete">
        delete from  cms_bt_images
        where id=#{id}

    </delete>

    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO cms_bt_images
        (
              `channel_id`,
              `code`,
              `original_url`,
              `img_name`,
              `upd_flg`,
              `created`,
              `creater`,
              `modified`,
              `modifier`
        )
        VALUES(
              #{channelId},
              #{code},
              #{originalUrl},
              #{imgName},
              #{updFlg},
              #{created},
              #{creater},
              #{modified},
              #{modifier}
        )
    </insert>
</mapper>