<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.voyageone.service.dao.jumei.CmsMtJmMasterPlatDao">
    <resultMap id="ResultMap" type="CmsMtJmMasterPlatModel">
        
    </resultMap>
	
    <select id="selectList" resultMap="ResultMap">

        select id,code,key,name1,name2,name3,name4,created,creater,modified,modifier        from cms_mt_jm_master_plat
    </select>
    <select id="select" resultMap="ResultMap">
        select id,code,key,name1,name2,name3,name4,created,creater,modified,modifier from cms_mt_jm_master_plat where id=#{id}
    </select>

     <update id="update">
        update cms_mt_jm_master_plat
        set

            `code`=#{code}
            ,`key`=#{key}
            ,`name1`=#{name1}
            ,`name2`=#{name2}
            ,`name3`=#{name3}
            ,`name4`=#{name4}
            ,`modified`=#{modified}
            ,`modifier`=#{modifier}
         where id=#{id}   
   </update>

    <delete id="delete">
        delete from  cms_mt_jm_master_plat
        where id=#{id}

    </delete>

    <insert id="insert" useGeneratedKeys="true" keyProperty="id" >
        INSERT INTO cms_mt_jm_master_plat
        (
              `code`,
              `key`,
              `name1`,
              `name2`,
              `name3`,
              `name4`,
              `created`,
              `creater`,
              `modified`,
              `modifier`
        )
        VALUES(
              #{code},
              #{key},
              #{name1},
              #{name2},
              #{name3},
              #{name4},
              #{created},
              #{creater},
              #{modified},
              #{modifier}
        )
    </insert>
</mapper>