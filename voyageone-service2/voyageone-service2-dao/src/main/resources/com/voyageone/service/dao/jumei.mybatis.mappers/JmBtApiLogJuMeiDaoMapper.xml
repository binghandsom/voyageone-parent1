<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.voyageone.service.dao.jumei.JmBtApiLogJuMeiDao">
    <resultMap id="ResultMap" type="JmBtApiLog">
        
    </resultMap>
	
    <select id="getList" resultMap="ResultMap">

        select id,source_id,api_type,error_msg,error_code,remark        from jumei2.jm_bt_api_log
    </select>
    <select id="get" resultMap="ResultMap">
        select * from jumei2.jm_bt_api_log where id=#{id}
    </select>

     <update id="update">
        update jumei2.jm_bt_api_log
        set

            source_id=#{sourceId}
            ,api_type=#{apiType}
            ,error_msg=#{errorMsg}
            ,error_code=#{errorCode}
            ,remark=#{remark}
         where id=#{id}   
   </update>

    <delete id="delete">
        delete from  jumei2.jm_bt_api_log
        where id=#{id}

    </delete>

    <insert id="create">
        INSERT INTO jumei2.jm_bt_api_log
        (
              source_id,
              api_type,
              error_msg,
              error_code,
              remark
        )
        VALUES(
              #{sourceId},
              #{apiType},
              #{errorMsg},
              #{errorCode},
              #{remark}
        )
    </insert>
</mapper>