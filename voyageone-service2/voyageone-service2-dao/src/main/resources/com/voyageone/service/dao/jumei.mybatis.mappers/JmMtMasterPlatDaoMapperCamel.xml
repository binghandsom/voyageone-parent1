<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.voyageone.service.dao.jumei.JmMtMasterPlatDao">
    <resultMap id="ResultMap" type="com.voyageone.service.model.jumei.JmMtMasterPlatModel">
        
    </resultMap>
	
    <select id="getList" resultMap="ResultMap">

        select id,code,key,name1,name2,name3,name4,created,creater,modified,modifier        from jumei2.jm_mt_master_plat
    </select>
    <select id="get" resultMap="ResultMap">
        select * from jumei2.jm_mt_master_plat where id=#{id}
    </select>

     <update id="update">
        update jumei2.jm_mt_master_plat
        set

            code=#{code}
            ,key=#{key}
            ,name1=#{name1}
            ,name2=#{name2}
            ,name3=#{name3}
            ,name4=#{name4}
            ,modified=#{modified}
            ,modifier=#{modifier}
         where id=#{id}   
   </update>

    <delete id="delete">
        delete from  jumei2.jm_mt_master_plat
        where id=#{id}

    </delete>

    <insert id="create">
        INSERT INTO jumei2.jm_mt_master_plat
        (
              code,
              key,
              name1,
              name2,
              name3,
              name4,
              created,
              creater,
              modified,
              modifier
        )
        VALUES(
              #{code},
              #{key},
              #{name1},
              #{name2},
              #{name3},
              #{name4},
              #{created},
              #{creater},
              #{modified},
              #{modifier}
        )
    </insert>
</mapper>