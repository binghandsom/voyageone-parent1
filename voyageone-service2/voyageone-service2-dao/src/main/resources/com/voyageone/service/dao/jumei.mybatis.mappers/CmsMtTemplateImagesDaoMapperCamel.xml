<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.voyageone.service.dao.jumei.CmsMtTemplateImagesDao">
    <resultMap id="ResultMap" type="CmsMtTemplateImagesModel">
        
    </resultMap>
	
    <select id="selectList" resultMap="ResultMap">

        select id,platform_id,channel_id,template_type,image_template_url,syn_flg,created,creater,modified,modifier        from cms_mt_template_images
    </select>
    <select id="select" resultMap="ResultMap">
        select id,platform_id,channel_id,template_type,image_template_url,syn_flg,created,creater,modified,modifier from cms_mt_template_images where id=#{id}
    </select>

     <update id="update">
        update cms_mt_template_images
        set

            `platform_id`=#{platformId}
            ,`channel_id`=#{channelId}
            ,`template_type`=#{templateType}
            ,`image_template_url`=#{imageTemplateUrl}
            ,`syn_flg`=#{synFlg}
            ,`modified`=#{modified}
            ,`modifier`=#{modifier}
         where id=#{id}   
   </update>

    <delete id="delete">
        delete from  cms_mt_template_images
        where id=#{id}

    </delete>

    <insert id="insert" useGeneratedKeys="true" keyProperty="id" >
        INSERT INTO cms_mt_template_images
        (
              `platform_id`,
              `channel_id`,
              `template_type`,
              `image_template_url`,
              `syn_flg`,
              `created`,
              `creater`,
              `modified`,
              `modifier`
        )
        VALUES(
              #{platformId},
              #{channelId},
              #{templateType},
              #{imageTemplateUrl},
              #{synFlg},
              #{created},
              #{creater},
              #{modified},
              #{modifier}
        )
    </insert>
</mapper>