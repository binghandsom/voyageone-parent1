<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.voyageone.service.daoext.jumei.CmsBtJmProductDaoExt">
<resultMap id="ResultMap" type="com.voyageone.service.model.jumei.CmsBtJmProductModel">
</resultMap>
<select id="getByProductCodeChannelId"  resultType="CmsBtJmProductModel">
    select  *   from cms_bt_jm_product where channel_id=#{channelId} and product_code=#{productCode} limit 1
</select>
</mapper>