<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.voyageone.service.dao.jumei.CmsBtJmApiLogDao">
    <resultMap id="ResultMap" type="CmsBtJmApiLogModel">
        
    </resultMap>
	
    <select id="selectList" resultMap="ResultMap">

        select id,source_id,api_type,error_msg,error_code,remark        from cms_bt_jm_api_log
        <where>
                <if test="id != null">
                        AND id = #{id}
                    </if>
                                <if test="sourceId != null">
                        AND source_id = #{sourceId}
                    </if>
                                <if test="apiType != null">
                        AND api_type = #{apiType}
                    </if>
                                <if test="errorMsg != null">
                        AND error_msg = #{errorMsg}
                    </if>
                                <if test="errorCode != null">
                        AND error_code = #{errorCode}
                    </if>
                                <if test="remark != null">
                        AND remark = #{remark}
                    </if>
                        </where>
    </select>
    <select id="selectOne" resultMap="ResultMap">
        select id,source_id,api_type,error_msg,error_code,remark from cms_bt_jm_api_log 
        <where>
                
                    <if test="id != null">
                        AND id = #{id}
                    </if> 
                
                    <if test="sourceId != null">
                        AND source_id = #{sourceId}
                    </if> 
                
                    <if test="apiType != null">
                        AND api_type = #{apiType}
                    </if> 
                
                    <if test="errorMsg != null">
                        AND error_msg = #{errorMsg}
                    </if> 
                
                    <if test="errorCode != null">
                        AND error_code = #{errorCode}
                    </if> 
                
                    <if test="remark != null">
                        AND remark = #{remark}
                    </if> 
        </where> limit 1
    </select>
    <select id="select" resultMap="ResultMap">
        select id,source_id,api_type,error_msg,error_code,remark from cms_bt_jm_api_log where id=#{id}
    </select>

     <update id="update">
        update cms_bt_jm_api_log
        set

            `source_id`=#{sourceId}
            ,`api_type`=#{apiType}
            ,`error_msg`=#{errorMsg}
            ,`error_code`=#{errorCode}
            ,`remark`=#{remark}
         where id=#{id}   
   </update>

    <delete id="delete">
        delete from  cms_bt_jm_api_log
        where id=#{id}

    </delete>

    <insert id="insert" useGeneratedKeys="true" keyProperty="id" >
        INSERT INTO cms_bt_jm_api_log
        (
              `source_id`,
              `api_type`,
              `error_msg`,
              `error_code`,
              `remark`
        )
        VALUES(
              #{sourceId},
              #{apiType},
              #{errorMsg},
              #{errorCode},
              #{remark}
        )
    </insert>
</mapper>