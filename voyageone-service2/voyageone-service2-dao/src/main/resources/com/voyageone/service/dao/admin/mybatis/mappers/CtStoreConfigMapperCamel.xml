<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.voyageone.service.dao.admin.CtStoreConfigDao" >
    <resultMap id="BaseResultMap" type="com.voyageone.service.model.admin.CtStoreConfigModel" />
    <insert id="insert" parameterType="com.voyageone.service.model.admin.CtStoreConfigModel" >
        insert into ct_store_config
        <trim prefix="(" suffix=")" suffixOverrides="," >
            <if test="storeId != null" >
                `store_id`,
            </if>
            <if test="cfgName != null" >
                `cfg_name`,
            </if>
            <if test="cfgVal1 != null" >
                `cfg_val1`,
            </if>
            <if test="cfgVal2 != null" >
                `cfg_val2`,
            </if>
            <if test="comment != null" >
                `comment`,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides="," >
            <if test="storeId != null" >
                #{storeId,jdbcType=BIGINT},
            </if>
            <if test="cfgName != null" >
                #{cfgName,jdbcType=VARCHAR},
            </if>
            <if test="cfgVal1 != null" >
                #{cfgVal1,jdbcType=VARCHAR},
            </if>
            <if test="cfgVal2 != null" >
                #{cfgVal2,jdbcType=VARCHAR},
            </if>
            <if test="comment != null" >
                #{comment,jdbcType=VARCHAR},
            </if>
        </trim>
    </insert>

</mapper>