<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.voyageone.service.daoext.cms.CmsBtSizeChartImageGroupDaoExt">
    <delete id="deleteByCmsBtSizeChartId">
           delete from cms_bt_size_chart_image_group where channel_Id=#{channelId} and cms_bt_size_chart_Id=#{cmsBtSizeChartId}
    </delete>

    <delete id="deleteByCmsBtImageGroupId">
            delete from cms_bt_size_chart_image_group where channel_Id=#{channelId} cms_bt_image_group_id=#{cmsBtImageGroupId}
    </delete>
</mapper>