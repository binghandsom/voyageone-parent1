<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
        xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.voyageone</groupId>
        <artifactId>voyageone-service2</artifactId>
        <version>2.0.0-SNAPSHOT</version>
    </parent>
    <artifactId>service2-api</artifactId>
    <version>${voyageone.service2.api.version}</version>
    <name>VoyageOne Service 2 - Interface</name>
    <dependencies>
        <dependency>
            <groupId>com.voyageone</groupId>
            <artifactId>service2-model</artifactId>
            <version>${voyageone.service2.model.version}</version>
        </dependency>
    </dependencies>
    <build>
        <finalName>voyageone-service2-api</finalName>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>${jdk.version}</source>
                    <target>${jdk.version}</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>

